  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Restrictions on external classes
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <!-- In this vocabulary foaf:Person has exactly one father and one mother -->
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            <owl:onProperty rdf:resource="http://purl.org/vocab/bio/0.1/father"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            <owl:onProperty rdf:resource="http://purl.org/vocab/bio/0.1/mother"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>


  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Classes relating to events
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Event">
    <rdfs:label xml:lang="en">Event</rdfs:label>
    <rdfs:comment xml:lang="en">An event is an occurrence that brings about a change in the state of affairs for one or more people and/or other agents. Events are assumed to occur over a period of time and may not have precise start and end points.</rdfs:comment>
    <vann:usageNote xml:lang="en">This class is intended to describe biographical events, i.e. events in the life of a person.</vann:usageNote>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/NET/c4dm/event.owl#Event" />
    <rdfs:subClassOf rdf:resource="http://sw.opencyc.org/2009/04/07/concept/en/Event" />
    <rdfs:subClassOf rdf:resource="http://linkedevents.org/ontology/Event" />
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/dcmitype/Event" />
    <skos:historyNote rdf:value="Made equivalent to cyc:Event" dc:date="2010-05-04" dc:creator="Ian Davis"/>
    <st:term_status>testing</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
    <skos:changeNote rdf:value="Marked as testing status" dc:date="2010-05-20" dc:creator="Ian Davis"/>
    <skos:historyNote rdf:value="Removed subclass of owl-time ProperInteval" dc:date="2010-05-20" dc:creator="Ian Davis"/>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/IndividualEvent">
    <rdfs:label xml:lang="en">Individual Event</rdfs:label>
    <rdfs:comment xml:lang="en">A type of event that is principally about a single person, group or organization. Other agents may be involved but the event is most significant for the principal agent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://purl.org/vocab/bio/0.1/principal"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/GroupEvent">
    <rdfs:label xml:lang="en">Group Event</rdfs:label>
    <rdfs:comment xml:lang="en">A type of event that is principally about one or more agents and their partnership. Other agents may be involved but the event is most significant for the partner agent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minCardinality>
        <owl:onProperty rdf:resource="http://purl.org/vocab/bio/0.1/partner"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>




  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Birth">
    <rdfs:label xml:lang="en">Birth</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person entering into life.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>testing</st:term_status>
    <skos:changeNote rdf:value="Marked as testing" dc:date="2010-05-04" dc:creator="Ian Davis"/>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>The birth of Albert Einstein</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Birth 
    ; dc:date "1879-03-14"
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:parent &lt;http://dbpedia.org/resource/Pauline_Koch&gt;
    ; bio:parent &lt;http://dbpedia.org/resource/Hermann_Einstein&gt;
    ; bio:place &lt;http://dbpedia.org/resource/Ulm&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Marriage">
    <rdfs:label xml:lang="en">Marriage</rdfs:label>
    <rdfs:comment xml:lang="en">The event of creating uniting the participants into a new family unit, sometimes accompanied by a formal wedding ceremony. This is intended to cover a broad range of marriages including those given formal legal standing, common-law, or by convention. It is not restricted to marriages of two people of the opposite gender, but also includes polygamous and same-sex unions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/GroupEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
    <owl:equivalentClass rdf:resource="http://sw.opencyc.org/2009/04/07/concept/en/WeddingEvent_Generic" />
    <skos:historyNote rdf:value="Made equivalent to cyc:WeddingEvent_Generic" dc:date="2010-05-04" dc:creator="Ian Davis"/>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>The marriage of Albert Einstein and Mileva MariÄ</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Marriage 
    ; dc:date "1903"
    ; bio:partner &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:partner &lt;http://dbpedia.org/resource/Mileva_Mari%C4%87&gt;
    ; bio:place &lt;http://dbpedia.org/resource/Bern&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>

  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Death">
    <rdfs:label xml:lang="en">Death</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person's life ending.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>The death of Albert Einstein</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Death 
    ; dc:date "1955-04-18"
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:place &lt;http://dbpedia.org/resource/Princeton,_New_Jersey&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>

  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Annulment">
    <rdfs:label xml:lang="en">Annulment</rdfs:label>
    <rdfs:comment xml:lang="en">The event of declaring a marriage void from the beginning as though it never existed.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/GroupEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Divorce">
    <rdfs:label xml:lang="en">Divorce</rdfs:label>
    <rdfs:comment xml:lang="en">The event of legally dissolving a marriage.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/GroupEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>The divorce of Albert Einstein and Mileva MariÄ</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Divorce 
    ; dc:date "1919-02-14"
    ; bio:partner &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:partner &lt;http://dbpedia.org/resource/Mileva_Mari%C4%87&gt;
    ; bio:place &lt;http://dbpedia.org/resource/Zurich&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Funeral">
    <rdfs:label xml:lang="en">Funeral</rdfs:label>
    <rdfs:comment xml:lang="en">The event of marking a person's death with a ceremony.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Burial">
    <rdfs:label xml:lang="en">Burial</rdfs:label>
    <rdfs:comment xml:lang="en">The event of interring the remains of a person's body into the ground.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Cremation">
    <rdfs:label xml:lang="en">Cremation</rdfs:label>
    <rdfs:comment xml:lang="en">The event of disposing of the remains of a person's body by fire.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>The cremation of Albert Einstein</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Cremation 
    ; dc:date "1955-04-18"
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:place &lt;http://dbpedia.org/resource/Princeton,_New_Jersey&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Adoption">
    <rdfs:label xml:lang="en">Adoption</rdfs:label>
    <rdfs:comment xml:lang="en">The event of creating of a legal parent/child relationship that does not exist biologically.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Baptism">
    <rdfs:label xml:lang="en">Baptism</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event held to admit a person to membership of a Christian church.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <skos:changeNote rdf:value="Revised comment, replacing 'the Christian church' with 'a Christian church' " dc:date="2010-05-20" dc:creator="Ian Davis"/>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/BarMitzvah">
    <rdfs:label xml:lang="en">BarMitzvah</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event held when a Jewish boy reaches age 13.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/BasMitzvah">
    <rdfs:label xml:lang="en">BasMitzvah</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event held when a Jewish girl reaching age 13, also known as "Bat Mitzvah."</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Execution">
    <rdfs:label xml:lang="en">Execution</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person being deliberately killed as punishment.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/Death"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-05</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Assassination">
    <rdfs:label xml:lang="en">Assassination</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person being deliberately targeted and killed.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/Murder"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-05</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Murder">
    <rdfs:label xml:lang="en">Murder</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person being killed unlawfully with intent by the killer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/Death"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-05</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Ordination">
    <rdfs:label xml:lang="en">Ordination</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event held when a person receives authority to act in religious matters.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Naturalization">
    <rdfs:label xml:lang="en">Naturalization</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person obtaining citizenship. Note that the place the naturalization event occurs at may be different from the state the person is obtaining citizenship of. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>Albert Einstein becoming a citizen of the United States</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Emigration 
    ; dc:date "1940"
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:state &lt;http://dbpedia.org/resource/United_States&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>

  </owl:Class>



  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Emigration">
    <rdfs:label xml:lang="en">Emigration</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person leaving their homeland with the intent of residing elsewhere.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>

    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>Albert Einstein emigrating to the United States</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Emigration 
    ; dc:date "1933"^^xsd:gYear
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:state &lt;http://dbpedia.org/resource/United_States&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>


  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Enrolment">
    <rdfs:label xml:lang="en">Enrolment</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person initiating attendence to a school or other place of learning.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Graduation">
    <rdfs:label xml:lang="en">Graduation</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person being awarded educational diplomas or degrees.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
    <vann:example>
      <ov:SchemaExample>
        <rdfs:label>Albert Einstein receiving his Doctorate from the University of Zurich</rdfs:label>
        <rdfs:comment rdf:parseType="Literal">
        <pre><code>_:e a bio:Graduation 
    ; dc:date "1905"^^xsd:gYear
    ; bio:principal &lt;http://dbpedia.org/resource/Albert_Einstein&gt;
    ; bio:organization &lt;http://dbpedia.org/resource/University_of_Zurich&gt;
    .</code></pre>
        </rdfs:comment>
      </ov:SchemaExample>
    </vann:example>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Imprisonment">
    <rdfs:label xml:lang="en">Imprisonment</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person being detained in a jail or prison.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-04</dct:issued>
  </owl:Class>



  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Employment">
    <rdfs:label xml:lang="en">Employment</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person entering an occupational relationship with an employer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>
    


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/PositionChange">
    <rdfs:label xml:lang="en">Change of Position</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person changing the position they hold with an employer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/NameChange">
    <rdfs:label xml:lang="en">Change of Name</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person changing their name.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Promotion">
    <rdfs:label xml:lang="en">Promotion</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person changing the position they hold with an employer to one with more importance or responsibility.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/PositionChange"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Demotion">
    <rdfs:label xml:lang="en">Demotion</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person changing the position they hold with an employer to one with less importance or responsibility.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/PositionChange"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Retirement">
    <rdfs:label xml:lang="en">Retirement</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person exiting an occupational relationship with an employer after a qualifying time period. In many cultures retirement is expected and even required once the person reaches a particular age.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Resignation">
    <rdfs:label xml:lang="en">Resignation</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person voluntarily giving up or quitting their office or position.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Dismissal">
    <rdfs:label xml:lang="en">Dismissal</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person involuntarily giving up their office or position. Dismissal is often perceived to be the employee's fault and may be considered disgraceful.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Redundancy">
    <rdfs:label xml:lang="en">Redundancy</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person involuntarily giving up an office or position that is no longer needed. Redundancy is usually perceived to be the employer's fault and is usually due to conditions outside of the employee's control.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Investiture">
    <rdfs:label xml:lang="en">Investiture</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event of a person taking a public office or honour.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Inauguration">
    <rdfs:label xml:lang="en">Inauguration</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event marking the beginning of a person's term of office as a leader.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Accession">
    <rdfs:label xml:lang="en">Accession</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a person succeeding to the right to hold regal power. This event is often automatic on the death of the previous monarch and is usually followed by a coronation event.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Coronation">
    <rdfs:label xml:lang="en">Coronation</rdfs:label>
    <rdfs:comment xml:lang="en">The ceremonial event of a person being invested with regal power to become a monarch.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Formation">
    <rdfs:label xml:lang="en">Formation</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a group or organization coming into being.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2011-06-10</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Disbanding">
    <rdfs:label xml:lang="en">Disbanding</rdfs:label>
    <rdfs:comment xml:lang="en">The event of a group or organization being disbanded.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/IndividualEvent"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2011-06-10</dct:issued>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Performance">
    <rdfs:label xml:lang="en">Performance</rdfs:label>
    <rdfs:comment xml:lang="en">The event of an individual or a group of performers performing. Examples include concerts, plays, recitals, recordings, busking etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/bio/0.1/GroupEvent"/>
    <owl:equivalentClass rdf:resource="http://purl.org/ontology/mo/Performance"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2011-06-12</dct:issued>
  </owl:Class>


  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Interval classes
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Interval">
    <rdfs:label xml:lang="en">Interval</rdfs:label>
    <rdfs:comment xml:lang="en">A extended interval of time related to a particular state of affairs, such as the lifespan of a person or a period of employment.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-05-03</dct:issued>
  </owl:Class>


  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Relationship classes
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Relationship">
    <rdfs:label xml:lang="en">Agent Relationship</rdfs:label>
    <rdfs:comment xml:lang="en">A particular type of connection existing between people, groups or organizations related to or having dealings with each other.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <owl:equivalentClass rdf:resource="http://purl.org/vocab/relationship/Relationship"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2011-06-14</dct:issued>
  </owl:Class>


  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Properties of a person
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/olb">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty" />
    <rdfs:label xml:lang="en">One-line bio</rdfs:label>
    <rdfs:comment xml:lang="en">A one-line biography of the person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <skos:historyNote rdf:value="Declared that this is a datatype property" dc:date="2010-05-03" dc:creator="Ian Davis"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
  </rdf:Property>


  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/keywords">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty" />
    <rdfs:label xml:lang="en">Key Words</rdfs:label>
    <rdfs:comment xml:lang="en">A comma delimited list of key words that describe a person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
    <dct:issued>2003-07-28</dct:issued>
    <skos:historyNote rdf:value="Declared that this is a datatype property" dc:date="2010-05-03" dc:creator="Ian Davis"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/biography">
    <rdfs:label xml:lang="en">Biography</rdfs:label>
    <rdfs:comment xml:lang="en">An extended description or account of someone's life.</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <st:term_status>unstable</st:term_status>
    <dct:issued>2010-06-05</dct:issued>
    <vann:usageNote rdf:parseType="Literal"><p>It is expected that the value of this property is either a literal formatted with HTML markup or a pointer to an HTML document.</p></vann:usageNote>
  </rdf:Property>

  <!-- 
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
      Properties of an event
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 
  -->

  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/date">
    <rdfs:label xml:lang="en">Date</rdfs:label>
    <label:plural xml:lang="en">Dates</label:plural>
    <rdfs:comment xml:lang="en">The date at which an event occurred.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <vann:usageNote rdf:parseType="Literal"><p>The date should be formatted as specified in <a href="http://www.w3.org/TR/NOTE-datetime">ISO8601</a>.
    For example: 2003-03-15 corresponds to the 15th March 2003, and 2003-03-15T13:21-05:00 corresponds to 15th March 2003, 8:21 am, US Eastern Standard Time.</p></vann:usageNote>
    <skos:changeNote rdf:value="Added a plural label" dc:date="2010-05-03" dc:creator="Ian Davis"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/place">
    <rdfs:label xml:lang="en">Place</rdfs:label>
    <label:plural xml:lang="en">Places</label:plural>
    <rdfs:comment xml:lang="en">The place at which an event occurred.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <skos:changeNote rdf:value="Added a plural label" dc:date="2010-05-03" dc:creator="Ian Davis"/>
    <st:term_status>stable</st:term_status>
    <skos:changeNote rdf:value="Marked as a stable term" dc:date="2010-05-04" dc:creator="Ian Davis"/>
  </rdf:Property>


  <rdf:Property rdf:about="http://purl.org/vocab/bio/0.1/position">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty" />
    <rdfs:label xml:lang="en">Position</rdfs:label>
    <label:plural xml:lang="en">Positions</label:plural>
    <rdfs:comment xml:lang="en">The employment position or public office involved in an event.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/bio/0.1/agent"/>
